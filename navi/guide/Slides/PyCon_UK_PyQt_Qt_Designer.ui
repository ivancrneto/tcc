         <property name="code" >
          <string>from PyQt4.QtCore import *
from PyQt4.QtGui import *
 
class ColorButton(QToolButton):
 
    __pyqtSignals__ = ("colorChanged(QColor)",)
    
    def __init__(self, parent = None):
    
        QToolButton.__init__(self, parent)
        self.connect(self, SIGNAL("clicked()"), self.chooseColor)
        self._color = QColor()
    
    def chooseColor(self):
    
        rgba, valid = QColorDialog.getRgba(
            self._color.rgba(), self.parentWidget())
        if valid:
            color = QColor.fromRgba(rgba)
            self.setColor(color)
    
    def color(self):
    
        return self._color
    
    @pyqtSignature("QColor")
    def setColor(self, color):
    
        if color != self._color:
            self._color = color
            self.emit(SIGNAL("colorChanged(QColor)"), self._color)
            pixmap = QPixmap(self.iconSize())
            pixmap.fill(color)
            self.setIcon(QIcon(pixmap))
    
    color = pyqtProperty("QColor", color, setColor)</string>
